@page
@model anketdeneme.Pages.AdminHomeModel
@{
    Layout = "_Layout";
}
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Ad</th>
                <th scope="col">Soyad</th>
                <th scope="col">Email</th>
                <th scope="col">Departman</th>
                <th scope="col">Mesaj Kutusu</th>
                <th scope="col">Memnuniyet</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in @Model.Sorulars)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Ad</td>
                    <td>@item.Soyad</td>
                    <td>@item.Email</td>
                    <td>@item.Departman</td>
                    <td>@item.MesajKutusu</td>
                    <td>@item.Memnuniyet.MemnuniyetDerecesi</td>
                </tr>
            }
        </tbody>
    </table>
</div>